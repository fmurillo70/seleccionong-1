<p id="notice"><%= notice %></p>

<h1>Respuesta</h1>

<table>
  <thead>
    <tr>
      <th>Texto</th>
      <th>Puntaje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @respuesta.each do |respuestum| %>
      <tr>
        <td><%= respuestum.texto %></td>
        <td><%= respuestum.puntaje %></td>
        <td><%= link_to 'Show', respuestum %></td>
        <td><%= link_to 'Edit', edit_respuestum_path(respuestum) %></td>
        <td><%= link_to 'Destroy', respuestum, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Respuestum', new_respuestum_path %>
