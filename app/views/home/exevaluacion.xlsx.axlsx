wb = xlsx_package.workbook
wb.add_worksheet(name: "Products") do |sheet|
sheet.add_row [ "Correo electronico", "Tipo de usuario" , "numero de inicios de sesion", "Fecha del ultimo inicio de sesion" ]
  @users.each do |v|

    a = ""
    if v.esadmin
        a = "Administrador"
    end

    if v.esong
            a = "ONG"
    end
    if v.esvoluntario
            a = "Voluntario"
    end

    sheet.add_row [v.email, a , v.sign_in_count,  v.current_sign_in_at ]
  end
end