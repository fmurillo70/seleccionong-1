<p id="notice"><%= notice %></p>

<h1>Aptitudes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aptitudes.each do |aptitude| %>
      <tr>
        <td><%= aptitude.nombre %></td>
        <td><%= link_to 'Show', aptitude %></td>
        <td><%= link_to 'Edit', edit_aptitude_path(aptitude) %></td>
        <td><%= link_to 'Destroy', aptitude, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aptitude', new_aptitude_path %>
