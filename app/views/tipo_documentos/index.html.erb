<div class="ui container">

<p id="notice"><%= notice %></p>

<h1>Tipo Documentos</h1>

  <table class="ui unstackable table">
    <thead>
    <tr>

      <th>Tipo de Documento</th>
      <th>Mostrar</th>
      <th>Editar</th>
      <th>Eliminar</th>

    </tr>
    </thead>

    <tbody>
    <% @tipo_documentos.each do |tipo_documento| %>
        <tr>
          <td><%= tipo_documento.nombre %></td>
          <td><%= link_to 'Mostrar', tipo_documento %></td>
          <td><%= link_to 'Editar', edit_tipo_documento_path(tipo_documento) %></td>
          <td><%= link_to 'Eliminar', tipo_documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
  </table>



<br>

<%= link_to 'Nuevo Tipo Documento', new_tipo_documento_path %>
</div>