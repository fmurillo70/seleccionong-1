<p id="notice"><%= notice %></p>

<h1>Tipo Estado Civils</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_estado_civils.each do |tipo_estado_civil| %>
      <tr>
        <td><%= tipo_estado_civil.nombre %></td>
        <td><%= link_to 'Show', tipo_estado_civil %></td>
        <td><%= link_to 'Edit', edit_tipo_estado_civil_path(tipo_estado_civil) %></td>
        <td><%= link_to 'Destroy', tipo_estado_civil, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

