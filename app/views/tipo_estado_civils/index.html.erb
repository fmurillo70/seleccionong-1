<div class="ui container">

<p id="notice"><%= notice %></p>

<h1>Tipo De Estado Civil</h1>

  <table class="ui unstackable table">
    <thead>
    <tr>

      <th>Estado civil</th>
      <th>Mostrar</th>
      <th>Editar</th>
      <th>Eliminar</th>

    </tr>
    </thead>

    <tbody>
    <% @tipo_estado_civils.each do |tipo_estado_civil| %>
        <tr>
          <td><%= tipo_estado_civil.nombre %></td>
          <td><%= link_to 'Mostrar', tipo_estado_civil %></td>
          <td><%= link_to 'Editar', edit_tipo_estado_civil_path(tipo_estado_civil) %></td>
          <td><%= link_to 'Eliminar', tipo_estado_civil, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

<br>

<%= link_to 'Nuevo Tipo Estado Civil', new_tipo_estado_civil_path %>
</div>
