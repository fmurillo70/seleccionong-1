<p id="notice"><%= notice %></p>

<h1>Ubicacionviviendas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubicacionviviendas.each do |ubicacionvivienda| %>
      <tr>
        <td><%= ubicacionvivienda.nombre %></td>
        <td><%= link_to 'Show', ubicacionvivienda %></td>
        <td><%= link_to 'Edit', edit_ubicacionvivienda_path(ubicacionvivienda) %></td>
        <td><%= link_to 'Destroy', ubicacionvivienda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubicacionvivienda', new_ubicacionvivienda_path %>
