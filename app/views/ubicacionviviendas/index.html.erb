<div class="ui container">

<p id="notice"><%= notice %></p>

<h1>Ubicacion viviendas</h1>

  <table class="ui unstackable table">
    <thead>
    <tr>

      <th>Lugar</th>
      <th>Mostrar</th>
      <th>Editar</th>
      <th>Eliminar</th>

    </tr>
    </thead>

    <tbody>
    <% @ubicacionviviendas.each do |ubicacionvivienda| %>
        <tr>
          <td><%= ubicacionvivienda.nombre %></td>
          <td><%= link_to 'Mostrar', ubicacionvivienda %></td>
          <td><%= link_to 'Editar', edit_ubicacionvivienda_path(ubicacionvivienda) %></td>
          <td><%= link_to 'Eliminar', ubicacionvivienda, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
        </tr>
    <% end %>
    </tbody>

  </table>


<br>

<%= link_to 'Nueva Ubicacion vivienda', new_ubicacionvivienda_path %>
</div>
