<p id="notice"><%= notice %></p>

<h1>Jornadaestudios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jornadaestudios.each do |jornadaestudio| %>
      <tr>
        <td><%= jornadaestudio.nombre %></td>
        <td><%= link_to 'Show', jornadaestudio %></td>
        <td><%= link_to 'Edit', edit_jornadaestudio_path(jornadaestudio) %></td>
        <td><%= link_to 'Destroy', jornadaestudio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jornadaestudio', new_jornadaestudio_path %>
