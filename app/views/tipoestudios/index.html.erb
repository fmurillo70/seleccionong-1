<p id="notice"><%= notice %></p>

<h1>Tipoestudios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipoestudios.each do |tipoestudio| %>
      <tr>
        <td><%= tipoestudio.nombre %></td>
        <td><%= link_to 'Show', tipoestudio %></td>
        <td><%= link_to 'Edit', edit_tipoestudio_path(tipoestudio) %></td>
        <td><%= link_to 'Destroy', tipoestudio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipoestudio', new_tipoestudio_path %>
